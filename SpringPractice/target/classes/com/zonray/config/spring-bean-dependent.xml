<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="address1" class="com.zonray.core.Address1">
		<property name="city" value="Guntur" />
		<property name="state" value="AP" />
	</bean>
	<bean id="person1" class="com.zonray.core.Person1">
		<property name="number" value="123" />
		<property name="name" value="JJKumar" />
		<property name="address" ref="address1" />
	</bean>
</beans>
